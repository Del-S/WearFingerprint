<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:width="1px" android:color="@color/colorDivider" />
            <solid android:color="#FFDDDDDD" />

        </shape>
    </item>

    <item android:top="1px" android:bottom="1px">
        <shape
            android:shape="rectangle">
            <stroke android:width="1px" android:color="@color/colorBackgroundWhite" />
            <solid android:color="@color/colorBackgroundWhite" />
        </shape>
    </item>

</layer-list>